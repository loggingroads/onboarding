      <div class="wrap">
        <div class="row">
          <div class="col-sm-12">
            <ul class="c-routes text text-desc -light">
              <!-- Paths to set -->
              <li class="route">Participate</li>
              <li class="route"><%= link_to 'Tasks', tasks_path, {:class => 'route-link'} %></li>
              <li class="route">Task name</li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    <div class="wrap">
