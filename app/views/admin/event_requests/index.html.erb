<div class="index-title wrap">
  <h1>Event Requests</h1>
</div>

<%= form_tag admin_event_requests_path, method: 'get', id: "search-form" do %>
  <div class="search wrap">
    <%= text_field_tag :search, params[:search], class: 'search-input' %>
    <svg class="icon icon-search">
      <use xlink:href="#icon-search"></use>
    </svg>
    <%= submit_tag '', name: nil, class: 'hidden' %>
  </div>
  <div id="search-table"><%= render 'event_requests' %></div>
<% end %>
