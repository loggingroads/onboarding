<tr class="wrap-full text text-desc -dark" data-role="campaign<%= campaign.id %>" data-move-url="<%= move_admin_campaign_url(campaign) %>" data-position="<%= campaign.position %>">
  <td>
    <svg class="icon icon-Row-Dragger">
      <use xlink:href="#icon-Row-Dragger"></use>
    </svg>
  </td>
  <td class="text text-desc -dark"><%= campaign.start_date %></td>
  <td class="campaign-name text text-desc -dark"><%= link_to campaign.name, admin_campaign_path(campaign) %></td>
  <td class="text text-desc -dark"><%= campaign.htag %></td>
</tr>
