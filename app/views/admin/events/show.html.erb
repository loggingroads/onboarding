<p>
  <strong>Name:</strong>
  <%= @event.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @event.description %>
</p>

<p>
  <strong>Image:</strong>
  <%= image_tag @event.image.url(:medium) %>
</p>

<p>
  <strong>Url:</strong>
  <%= @event.url %>
</p>

<p>
  <strong>Date:</strong>
  <%= @event.date %>
</p>

<p>
  <strong>Location:</strong>
  <%= @event.location %>
</p>

<p>
  <strong>Instructions:</strong>
  <%= @event.instructions %>
</p>

<p>
  <strong>Contact:</strong>
  <%= @event.contact %>
</p>

<%= link_to 'Edit', edit_admin_event_path(@event), class: 'bttn bttn-primary' %>
<%= link_to 'Back', admin_events_path, class: 'bttn bttn-secondary' %>
