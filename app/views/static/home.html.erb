
<div id="parallax">
  <%= render 'header' %>
  <article class="intro" id="intro">
    <%= render 'intro' %>
  </article>
  <article class="slider" id="slider">
    <%= render 'slider' %>
    <div class="arrow-left"></div>
    <div class="arrow-right"></div>
  </article>
  <article class="participate" id="participate">
    <%= render 'participate' %>
  </article>
  <article class="events" id="events">
    <%= render 'events' %>
  </article>
  <article class="community" id="community">
    <%= render 'community' %>
  </article>
  <article class="activity" id="activity">
    <%= render 'activity' %>
  </article>
  <article class="about" id="about">
