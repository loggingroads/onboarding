    <div id="slider-dot" class="dot-item"><a href="#slider" title="Collaborate">&nbsp</a></div>
    <div id="participate-dot" class="dot-item"><a href="#participate" title="Participate">&nbsp</a></div>
    <div id="events-dot" class="dot-item"><a href="#events" title="Events">&nbsp</a></div>
